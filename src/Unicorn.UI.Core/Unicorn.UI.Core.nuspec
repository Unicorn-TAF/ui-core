<?xml version="1.0"?>
<package >
  <metadata>
    <id>Unicorn.UI.Core</id>
    <version>$version$</version>
    <title>Unicorn.UI.Core</title>
    <authors>Vitaliy Dobriyan</authors>
    <license type="expression">Apache-2.0</license>
    <copyright>Copyright (c) Vitaliy Dobriyan 2024</copyright>
    <projectUrl>https://unicorn-taf.github.io</projectUrl>
    <repository type="git" url="https://github.com/Unicorn-TAF/ui-core"/>
    <icon>images\icon.png</icon>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      Unicorn.UI.Core is extension of Unicorn test automation framework which allows to use in tests implementations of specific UI extensions or develop custom ones.
      Unicorn UI Core features: Common controls interfaces, UI matchers for common controls, synchronization methods (waits), core for different UI implementations.
      This package includes UI core of unicorn automation framework.
    </description>
    <releaseNotes>Support of Taf.Core 3.2.0, legacy logger replaced with new one, implemented non-generic alternatives for Find (returns IControl) and TryGetChild.</releaseNotes>
    <tags>unicorn-taf automation ui matchers</tags>
    <dependencies>
      <group targetFramework="net452">
        <dependency id="Unicorn.Taf.Core" version="3.2.0"/>
      </group>
      <group targetFramework="net5.0">
        <dependency id="Unicorn.Taf.Core" version="3.2.0"/>
      </group>
      <group targetFramework="netstandard2.0">
        <dependency id="Unicorn.Taf.Core" version="3.2.0"/>
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\icon.png" target="images\" />
    <file src="bin\Release\net452\unicorn.ui.core.dll" target="lib\net452" />
    <file src="bin\Release\net452\unicorn.ui.core.xml" target="lib\net452" />
    <file src="bin\Release\netstandard2.0\unicorn.ui.core.dll" target="lib\netstandard2.0" />
    <file src="bin\Release\netstandard2.0\unicorn.ui.core.xml" target="lib\netstandard2.0" />
    <file src="bin\Release\net5.0\unicorn.ui.core.dll" target="lib\net5.0" />
    <file src="bin\Release\net5.0\unicorn.ui.core.xml" target="lib\net5.0" />
  </files>
</package>